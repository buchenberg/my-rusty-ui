{
  "name": "my-rusty-ui",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "postbuild": "npm run build-bulma",
    "lint": "eslint .",
    "preview": "vite preview",
    "build-bulma": "sass --load-path=node_modules src/index.scss src/index.css",
    "watch-bulma": "sass --watch --load-path=node_modules my-rusty-ui.scss my-rusty-ui.css"
  },
  "dependencies": {
    "bulma": "^1.0.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "zustand": "^5.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "eslint": "^9.25.0",
    "sass": "^1.89.1",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.30.1",
    "vite": "^6.3.5"
  }
}